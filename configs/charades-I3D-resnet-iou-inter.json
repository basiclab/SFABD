{
    "num_init_clips": 32,
    "num_clips": 16,
    "TrainDataset": "src.datasets.charades.CharadesSTAI3DTrain",
    "TestDataset": "src.datasets.charades.CharadesSTAI3DTest",
    "MultiTestDataset": "src.datasets.charades.CharadesSTAI3DMultiTest",

    "backbone": "src.models.resnet.ProposalConv",
    "feat1d_out_channel": 512,
    "feat1d_pool_kernel_size": 2,
    "feat2d_pool_counts": [16],
    "conv2d_hidden_channel": 512,
    "conv2d_kernel_size": 5,
    "conv2d_num_layers": 8,
    "joint_space_size": 256,
    "dual_space": false,
    "resnet": 50,

    "IoULoss": "src.losses.iou.ScaledIoULoss",
    "min_iou": 0.5,
    "max_iou": 1.0,
    "alpha": 0.25,
    "gamma": 2.0,
    "iou_weight": 1.0,

    "iou_threshold": 0.75,
    "neg_iou": 0.5,
    "pos_topk": 1,
    "contrastive_decay": 0.1,
    "contrastive_decay_start": 8,

    "InterContrastiveLoss": "src.losses.contrastive.InterContrastiveLoss",
    "inter_t": 0.1,
    "inter_m": 0.4,
    "inter_weight": 0.05,

    "IntraContrastiveLoss": "src.losses.contrastive.IntraContrastiveLoss",
    "intra_t": 0.1,
    "intra_m": 0.0,
    "intra_weight": 0.0,
    "do_augmentation": false,

    "epochs": 12,
    "batch_size": 48,
    "base_lr": 1e-4,
    "bert_lr": 1e-5,
    "milestones": [8, 13],
    "step_gamma": 0.1,
    "bert_fire_start": 1,
    "grad_clip": 5.0,

    "best_metric": "R@1,IoU=0.7",
    "logdir": "./logs/charades-I3D-resnet-iou-inter"
}
