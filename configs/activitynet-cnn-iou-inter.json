{
    "num_init_clips": 256,
    "num_clips": 64,
    "TrainDataset": "src.datasets.activitynet.ActivityNetTrain",
    "TestDataset": "src.datasets.activitynet.ActivityNetTest",

    "backbone": "src.models.cnn.ProposalConv",
    "feat1d_out_channel": 512,
    "feat1d_pool_kernel_size": 4,
    "feat2d_pool_counts": [16, 8, 8],
    "conv2d_hidden_channel": 512,
    "conv2d_kernel_size": 9,
    "conv2d_num_layers": 4,
    "joint_space_size": 256,

    "IoULoss": "src.losses.iou.ScaledIoULoss",
    "min_iou": 0.5,
    "max_iou": 1.0,
    "alpha": 0.25,
    "gamma": 2.0,
    "iou_weight": 1.0,

    "iou_threshold": 0.75,
    "bbox_reg_weight": 1.0,

    "neg_iou": 0.5,
    "pos_topk": 1,
    "contrastive_decay": 0.01,
    "contrastive_decay_start": 7,

    "InterContrastiveLoss": "src.losses.contrastive.InterContrastiveLoss",
    "inter_t": 0.1,
    "inter_m": 0.3,
    "inter_weight": 0.1,

    "IntraContrastiveLoss": "src.losses.contrastive.IntraContrastiveLoss",
    "intra_t": 0.1,
    "intra_m": 0.0,
    "intra_weight": 0.0,

    "epochs": 18,
    "batch_size": 48,
    "base_lr": 8e-4,
    "bert_lr": 8e-5,
    "milestones": [7, 15],
    "step_gamma": 0.1,
    "bert_fire_start": 5,
    "grad_clip": 5.0,

    "logdir": "./logs/activitynet-cnn-iou-inter"
}
