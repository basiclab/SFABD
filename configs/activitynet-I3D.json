{
    "num_init_clips": 128,
    "num_clips": 64,
    "TrainDataset": "src.datasets.activitynet.ActivityNetI3DTrain",
    "ValDataset": "src.datasets.activitynet.ActivityNetI3DVal",
    "TestDataset": "src.datasets.activitynet.ActivityNetI3DTest",
    "MultiTestDataset": "src.datasets.activitynet.ActivityNetI3DMultiTest",

    "backbone": "src.models.resnet.ProposalConv",
    "feat1d_out_channel": 256,
    "feat1d_pool_kernel_size": 2,
    "feat2d_pool_counts": [16, 8, 8],
    "conv2d_hidden_channel": 256,
    "conv2d_kernel_size": 9,
    "conv2d_num_layers": 4,
    "joint_space_size": 256,
    "dual_space": true,
    "resnet": 18,
    "contrastive_decay": 0.01,
    "contrastive_decay_start": 7,

    "IoULoss": "src.losses.iou.ScaledIoULossAFND",
    "min_iou": 0.5,
    "max_iou": 1.0,
    "iou_weight": 1.0,
    "neg_iou": 0.5,
    "pos_topk": 1,

    "InterContrastiveLoss": "src.losses.contrastive.InterContrastiveLossAFND",
    "inter_weight": 0.05,
    "inter_t": 0.1,
    "inter_m": 0.3,

    "IntraContrastiveLoss": "src.losses.contrastive.IntraContrastiveLossAFND",
    "intra_weight": 0.01,
    "intra_t": 0.05,
    "intra_m": 0.2,

    "do_augmentation": true,
    "aug_prob": 0.25,
    "downsampling_prob": 0.0,
    "mixup_alpha": 0.9,
    "downsampling_method": "odd",

    "do_afnd": true,
    "thres_method": "max",
    "accept_rate_method": "linear0.5x",

    "epochs": 8,
    "batch_size": 16,
    "test_batch_size": 48,
    "base_lr": 1e-3,
    "bert_lr": 5e-5,
    "milestones": [8],
    "step_gamma": 0.1,
    "bert_fire_start": 5,
    "grad_clip": 5.0,
    "seed": 0,

    "best_metric": "R@1,IoU=0.7",
    "logdir": "./logs/activitynet-I3D-best"
}
