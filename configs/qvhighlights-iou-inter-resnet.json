{
    "TrainDataset": "src.datasets.qvhighlights.QVHighlightsTrain",
    "TestDataset": "src.datasets.qvhighlights.QVHighlightsVal",
    "num_init_clips": 128,
    "num_clips": 64,
    "feat1d_out_channel": 512,
    "feat1d_pool_kernel_size": 2,
    "feat2d_pool_counts": [16, 8, 8],
    "conv2d_hidden_channel": 512,
    "conv2d_kernel_size": 9,
    "conv2d_num_layers": 4,
    "joint_space_size": 256,

    "min_iou": 0.5,
    "max_iou": 1.0,
    "iou_weight": 1.0,

    "iou_threshold": 0.75,
    "bbox_reg_weight": 1.0,

    "tau_video": 0.1,
    "tau_query": 0.1,
    "neg_iou": 0.5,
    "pos_topk": 1,
    "margin": 0.3,
    "intra_start": 0,
    "contrastive_decay": 0.1,
    "contrastive_decay_start": 5,
    "inter_weight": 10,
    "intra_weight": 0,

    "epochs": 10,
    "batch_size": 24,
    "base_lr": 1e-3,
    "bert_lr": 1e-5,
    "milestones": [10],
    "step_gamma": 0.1,
    "bert_freeze_epoch": 5,
    "grad_clip": 5.0,

    "test_batch_size": 48,
    "nms_threshold": 0.5,
    "recall_Ns": [1, 5],
    "recall_IoUs": [0.5, 0.7],
    "logdir": "./logs/qvhighlights-iou-inter-resnet"
}
